# .env.example

# ========== ALPACA API ==========
# Sign up: https://app.alpaca.markets/
# Get keys from: Settings → API Keys
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET=your_alpaca_secret_here

# ========== CONFLUENT CLOUD ==========
# Sign up: https://confluent.cloud/
# Get cluster API key: Cluster → API Keys → Create Key
CONFLUENT_SERVER=pkc-xxx.region.provider.confluent.cloud:9092
CONFLUENT_API_KEY=your_confluent_api_key_here
CONFLUENT_API_SECRET=your_confluent_api_secret_here

# Get Schema Registry key: Cluster → Schema Registry → API Keys
SCHEMA_REGISTRY_URL=https://psrc-xxx.region.provider.confluent.cloud
SR_API_KEY=your_sr_api_key_here
SR_API_SECRET=your_sr_api_secret_here

# Get the URL from Confluent cloud
KSQLDB_URL=your-url